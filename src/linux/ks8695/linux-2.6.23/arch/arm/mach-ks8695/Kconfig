if ARCH_KS8695

choice
    prompt "Kendin/Micrel KS8695 Implementations"

config MACH_KS8695
	bool "KS8695 development board"
	help
	  Say 'Y' here if you want your kernel to run on the original
	  Kendin-Micrel KS8695 development board.

config ARCH_KS8695_VSOPENRISC
	bool "VScom OpenRISC platform"
	select ISA
	help
	  Say 'Y' here if you want your kernel to run on the OpenRISC
	  platform. <http://www.vscom.de/>

endchoice

endif
